{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "cybrance",
  "name": "Cybrance.nl Docs",
  "colors": {
    "primary": "#5865F2",
    "light": "#7289DA",
    "dark": "#23272A"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/settings",
              "essentials/navigation",
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Introduction",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoints",
            "pages": [
              "api-reference/endpoint/create",
              "api-reference/endpoint/get",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Cybrance Website",
          "href": "https://cybrance.nl",
          "icon": "globe"
        },
        {
          "anchor": "Discord Community",
          "href": "https://discord.gg/cybrance",
          "icon": "discord"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@cybrance.nl"
      },
      {
        "label": "Discord",
        "href": "https://discord.gg/cybrance"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://cybrance.nl/dashboard"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "discord",
     "cybrance-api",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/cybrance",
      "github": "https://github.com/cybrance",
      "linkedin": "https://linkedin.com/company/cybrance"
    }
  }
}
